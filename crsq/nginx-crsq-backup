
server {

  listen 80;

  access_log /home/ubuntu/crsq-access.log;
  error_log /home/ubuntu/crsq-error.log error;

  # Make site accessible from http://localhost/
  server_name *.zippednews.com zippednews.com;


  location /robots.txt {
       alias /home/ubuntu/crsq/crsq/robots/robotszippednewsapp.txt;
   }

  location /sitemap.xml.gz {
	alias /home/ubuntu/crsq/crsq/robots/sitemapzippednewsapp.xml.gz;
   }

  location / {
   	    proxy_pass http://localhost:8000/zippednewsapp/;
  }

  location /static/ {

	gzip             on;
	gzip_http_version 1.1;
	gzip_vary on;
	gzip_comp_level 6;
	gzip_proxied any;
	gzip_types text/css text/javascript application/json application/javascript application/x-javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml;
	gzip_buffers 128 4k;
	gzip_disable "MSIE [1-6]\.(?!.*SV1)";

   	alias /home/ubuntu/crsq/crsq/static/;
  }


}

server {

  listen 80;

  access_log /home/ubuntu/crsq-access.log;
  error_log /home/ubuntu/crsq-error.log error;

  # Make site accessible from http://localhost/
  server_name *.penpatron.com penpatron.com;


  location /robots.txt {
       alias /home/ubuntu/crsq/crsq/robots/robotspenpatron.txt;
   }

  location / {
            proxy_pass http://localhost:8000/penpatron/;
  }

  location /static/ {

        gzip             on;
        gzip_http_version 1.1;
        gzip_vary on;
        gzip_comp_level 6;
        gzip_proxied any;
        gzip_types text/css text/javascript application/json application/javascript application/x-javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml;
        gzip_buffers 128 4k;
        gzip_disable "MSIE [1-6]\.(?!.*SV1)";

        alias /home/ubuntu/crsq/crsq/static/;
  }


}

server {

  listen 80;

  access_log /home/ubuntu/crsq-access.log;
  error_log /home/ubuntu/crsq-error.log error;

  # Make site accessible from http://localhost/
  server_name *.twitterstreetjournal.com twitterstreetjournal.com;


  location /robots.txt {
       alias /home/ubuntu/crsq/crsq/robots/robotstwitterstreetjournal.txt;
   }

  location / {
            proxy_pass http://localhost:8000/twitterstreetjournal/;
  }

  location /static/ {

        gzip             on;
        gzip_http_version 1.1;
        gzip_vary on;
        gzip_comp_level 6;
        gzip_proxied any;
        gzip_types text/css text/javascript application/json application/javascript application/x-javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml;
        gzip_buffers 128 4k;
        gzip_disable "MSIE [1-6]\.(?!.*SV1)";

        alias /home/ubuntu/crsq/crsq/static/;
  }


}

server {

  listen 80 default;

  access_log /home/ubuntu/crsq-access.log;
  error_log /home/ubuntu/crsq-error.log error;

  # Make site accessible from http://localhost/
  server_name crsq;

  #ssl_certificate /etc/nginx/ssl/server.crt;
  #ssl_certificate_key /etc/nginx/ssl/server.key;

  location /robots.txt {
       alias /home/ubuntu/crsq/crsq/robots.txt;
   }

   location ~ /zippednewsapp(.*) {
	   return 301 http://www.zippednews.com$1;
   }

   location ~ /penpatron(.*) {
           return 301 http://www.penpatron.com$1;
   }

   location / {
            proxy_pass http://localhost:8000/;
   }

  location /static/ {

        gzip             on;
        gzip_http_version 1.1;
        gzip_vary on;
        gzip_comp_level 6;
        gzip_proxied any;
        gzip_types text/css text/javascript application/json application/javascript application/x-javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml;
        gzip_buffers 128 4k;
        gzip_disable "MSIE [1-6]\.(?!.*SV1)";

        alias /home/ubuntu/crsq/crsq/static/;
  }


}


server {

  listen 443;
  ssl on;

  access_log /home/ubuntu/crsq-access.log;
  error_log /home/ubuntu/crsq-error.log error;

  # Make site accessible from http://localhost/
  server_name crsq;

  ssl_certificate /etc/nginx/ssl/server.crt;
  ssl_certificate_key /etc/nginx/ssl/server.key;

  location /robots.txt {
       alias /home/ubuntu/crsq/crsq/robots.txt;
   }

   location ~ /zippednewsapp(.*) {
           return 301 http://www.zippednews.com$1;
   }

   location ~ /penpatron(.*) {
           return 301 http://www.penpatron.com$1;
   }

   location / {
            proxy_pass http://localhost:8000/;
   }

   location /static/ {

        gzip             on;
        gzip_http_version 1.1;
        gzip_vary on;
        gzip_comp_level 6;
        gzip_proxied any;
        gzip_types text/css text/javascript application/json application/javascript application/x-javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml;
        gzip_buffers 128 4k;
        gzip_disable "MSIE [1-6]\.(?!.*SV1)";

        alias /home/ubuntu/crsq/crsq/static/;
  }


}

